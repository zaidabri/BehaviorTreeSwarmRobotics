<?xml version="1.0"?>
<root main_tree_to_execute="BehaviorTree">
    <!-- ////////// -->
    <BehaviorTree ID="BehaviorTree">
        <Sequence>
            <Sequence>
                <Condition ID="BatteryLessThan20"/>
                <Action ID="GoChargingStation"/>
            </Sequence>
            <Sequence>
                <Condition ID="OrderAvailable"/>
                <Action ID="ReserveOrder"/>
            </Sequence>
            <Sequence>
                <RetryUntilSuccessful num_attempts="5">
                    <Action ID="FindPickupStation"/>
                </RetryUntilSuccessful>
                <Fallback>
                    <Sequence>
                        <Condition ID="ObstacleFound"/>
                        <Action ID="RecalculateRoute"/>
                    </Sequence>
                    <Action ID="MoveForward"/>
                </Fallback>
                <Action ID="PickOrder"/>
            </Sequence>
            <Sequence>
                <RetryUntilSuccessful num_attempts="5">
                    <Action ID="FindTransferStation"/>
                </RetryUntilSuccessful>
                <Action ID="SendDeliverySignal" key_name="TS4"/>
                <Fallback>
                    <Sequence>
                        <Condition ID="ObstacleFound"/>
                        <Action ID="RecalculateRoute"/>
                    </Sequence>
                    <Action ID="MoveForward"/>
                </Fallback>
                <Action ID="DeliverOrder"/>
                <Action ID="SetStatusAsFree"/>
            </Sequence>
        </Sequence>
    </BehaviorTree>
    <!-- ////////// -->
    <TreeNodesModel>
        <Condition ID="BatteryLessThan20"/>
        <Action ID="DeliverOrder"/>
        <Action ID="FindPickupStation"/>
        <Action ID="FindTransferStation"/>
        <Action ID="GoChargingStation"/>
        <Action ID="MoveForward"/>
        <Condition ID="ObstacleFound"/>
        <Condition ID="OrderAvailable"/>
        <Action ID="PickOrder"/>
        <Action ID="RecalculateRoute"/>
        <Action ID="ReserveOrder"/>
        <Action ID="SendDeliverySignal">
            <output_port default="TS4" name="key_name"/>
        </Action>
        <Action ID="SetStatusAsFree"/>
    </TreeNodesModel>
    <!-- ////////// -->
</root>